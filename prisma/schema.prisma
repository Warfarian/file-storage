
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id String @id @default(cuid())
  sid String @unique
  data String
  expiresAt DateTime
}

model User {
  id String @id @unique
  username String @unique
  email String @unique
  password String
}